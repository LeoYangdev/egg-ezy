page{
  background-color: #f1f1f1;
}
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  height: 100%;
}

.icon {
  width: 300rpx;
  height: 300rpx;
  margin-bottom: 20rpx;
}
.icon image {
  width: 300rpx;
  height: 300rpx;
}

.title {
  font-size: 36rpx;
  font-weight: bold;
  margin-bottom: 20rpx;
}

.info {
  display: flex;
  flex-direction: column;
  align-items: flex-start;
  font-size: 32rpx;
  line-height: 50rpx;
  color: #797979;
}
